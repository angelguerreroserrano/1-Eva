# Este bucle imprime los números del 1 al 10
def bucle_1():
    for cont in range(1,11):
        print (cont)
bucle_1()

# Suma los 10 números que pongas
def bucle_1():
    suma = 0
    for cont in range(1,11):
        num = int(input("Dime un número: "))
        suma = num + suma
        print (suma)
bucle_1()
