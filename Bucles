# Este bucle imprime los números del 1 al 10
def bucle_1():
    for cont in range(1,11): # Esto te dice que se repita la acción 10 veces
        print (cont)
bucle_1()

# Suma los 10 números que pongas
def bucle_1():
    suma = 0
    for cont in range(1,11): 
        num = int(input("Dime un número: "))
        suma = num + suma
    print (suma)
bucle_1()

# Tabla de multiplicar del número que des
n = int(input("Dime la tabla de multiplicar que quieres que ponga: "))
def bucle_1():
    for cont in range(1,11):
        print (f"{n}*{cont}=", n*cont)        
bucle_1()
