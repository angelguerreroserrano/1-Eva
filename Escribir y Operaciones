# Para escribir algo
print ("Hola mundo")

# Para hacer operaciones
print (2+3)
print (2-3)
print (2*3)
print (2/3)

# a, b y c son variantes, que las defino al principio
a = 2
b = 3
c = a+b
print (c)

# Tambi√©n se puede sumar nombres, o multiplicarlos
a = Angel
b = Guerrero
print (a+b)
print (a*2)
